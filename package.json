  },
  "scripts": {
    "format": "prettier --write **/*.{js,s?css,yml} !dist",
    "start": "cross-env NODE_ENV=development run-p -l start:*",
    "start:webpack": "webpack --config webpack.dev.js ",
    "start:eleventy": "eleventy --serve",
    "update:data-test": "node ./scripts/update_data-test.js",
    "update:data": "node ./scripts/update_data.js",
    "build": "cross-env NODE_ENV=production run-s -l build:*",
    "build:clean": "rimraf dist",
    "build:webpack": "webpack --config webpack.prod.js",
    "build:eleventy": "eleventy",
    "analyze": "run-s build analyze:*",
    "analyze:js": "source-map-explorer dist/**/*.js dist/**/*.js.map"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "chokidar": "^3.4.2",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.2",
    "favicons-webpack-plugin": "^4.2.0",
    "file-loader": "^6.0.0",
    "firebase": "^7.23.0",
    "firebase-admin": "^9.2.0",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^4.3.0",
    "img-optimize-loader": "^1.0.7",
    "js-yaml": "^3.14.0",
    "mini-css-extract-plugin": "^0.11.0",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.2",
    "source-map-explorer": "^2.4.2",
    "watchpack": "^1.7.4",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.11",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^5.1.3"
  },
  "dependencies": {
    "animejs": "^3.2.0",
    "bulma": "^0.9.0",
    "swup": "^2.0.11"
  }
