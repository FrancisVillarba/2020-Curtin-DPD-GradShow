---
layout: default.njk
title: Major Page
pagination:
    data: majors
    size: 1
    alias: major
permalink: "major/{{ major.title | slug }}/"

--- 

<section class="banner">
  <div class="banner-text">
    <h3 class="major-intro">Students From The Major Of</h3>
    <h1 class="title major-title">{{ major.title }}</h1>
  </div>
  <img class="banner-img" src=".././imgs/orbs/dd.png"></img>
</section>
<section class="profile-listing-showreel-container">
</section>
<section class="py-6 px-6 mx-auto">
  <div class="search-bar-section mb-6">
    <div class="search-bar-wrapper">
      <div class="control has-icons-left">
          <input class="search-bar" type="text" placeholder="SEARCH">
          <span class="icon is-left search-icon">
            <i class="fas fa-search"></i>
          </span>
      </div>
    </div>
  </div>
  <div class="student-profiles student-profile-container">
  </div>
</section>

